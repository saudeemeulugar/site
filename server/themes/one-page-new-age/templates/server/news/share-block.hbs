      <a href="mailto:?subject={{data.title}} - {{req.we.systemSettings.siteName}}&amp;body={{data.about}} - Leia mais no link: {{req.we.config.hostname}}{{data.linkPermanent}}"
         title="Compartilhar por Email">
        <img src="/public/theme/one-page-new-age/icons/email.png" alt="Compartilhar por e-mail">
      </a>
